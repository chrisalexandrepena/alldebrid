{
  "name": "alldebrid",
  "type": "module",
  "version": "2.0.0",
  "author": "Chris-Alexandre Pena",
  "licence": "GPL-3.0",
  "description": "AllDebrid SDK + CLI in TypeScript",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "alldebrid": "./dist/cli/index.cjs"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm build && pnpm test"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18"
  },
  "volta": {
    "node": "22.18.0"
  },
  "dependencies": {
    "luxon": "^3.7.1",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.3.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vitest": "^3.2.4"
  },
  "keywords": [],
  "packageManager": "pnpm@10.15.0"
}
